{
  "name": "maofan-apn",
  "version": "1.3.0",
  "description": "APN service for Maofan 2",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LitoMore/maofan-apn.git"
  },
  "author": "LitoMore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LitoMore/maofan-apn/issues"
  },
  "homepage": "https://github.com/LitoMore/maofan-apn#readme",
  "dependencies": {
    "apn": "^2.2.0",
    "chalk": "^2.4.1",
    "fancy-log": "^1.3.0",
    "fanfou-streamer": "0.2.1",
    "koa": "^2.5.2",
    "koa-body": "^4.0.4",
    "koa-router": "^7.4.0",
    "log-symbols": "^2.2.0",
    "mongoose": "^5.2.6"
  },
  "devDependencies": {
    "xo": "^0.23.0"
  },
  "xo": {
    "rules": {
      "import/no-unresolved": 0,
      "space-before-function-paren": [
        "error",
        "always"
      ],
      "unicorn/catch-error-name": [
        "error",
        {
          "name": "err"
        }
      ]
    },
    "semicolon": false,
    "space": 2
  }
}
